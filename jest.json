{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "coveragePathIgnorePatterns": [
    "src/database/entities",
    "test/unit/api/vdo-call/dto",
    "^.*\\.config\\.[jt]s?$"
  ],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleDirectories": ["<rootDir>/../", "node_modules"],
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1"
  }
}
